from turtle import *
def Weihnachtsbaum(x,y):
  penup()
  goto(x,y)
  pendown()
  setheading(90)
  forward(30)
  left(90)
  forward(30)
  right(120)
  forward(60)
  right(120)
  forward(60)
  right(120)
  forward(30)

from turtle import *
shape('turtle')
for x in range(20):
  left(18)
  forward(20)

onclick(goto)
mainloop()
